         <rowConstraints>
            <RowConstraints maxHeight="192.0" minHeight="10.0" prefHeight="83.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="450.0" minHeight="10.0" prefHeight="402.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <AnchorPane id="dash-container" stylesheets="@style.css" GridPane.rowIndex="1">
               <children>
                  <VBox alignment="CENTER" layoutY="31.0" prefHeight="371.0" prefWidth="513.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="31.0">
