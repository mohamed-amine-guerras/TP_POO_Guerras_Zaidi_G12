                        </JFXButton>
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </VBox>
                  <ImageView fitHeight="97.0" fitWidth="254.0" layoutX="152.0" layoutY="-36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@/../../../../../../Documents/Pendu.png" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </children>
      </GridPane>
